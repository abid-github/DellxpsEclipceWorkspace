package com.stack;

import java.util.Stack;

public class ScoreOfParentheses {

	static int score(String s)

	{
		int sc = 0;

		Stack<Integer> st = new Stack<>();

		for (char c : s.toCharArray()) {

			if (c == '(')

			{
				st.push(sc);
				

			}
		}

		return sc;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
