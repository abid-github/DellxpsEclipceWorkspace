package com.algo.ds;

public class SumkOfSubArray {

	static int sum(int[] a, int k) {

		int res = 0;

		int sum = 0;

		for (int i = 0; i < a.length; i++)

		{
			sum = a[i];

			for (int j = i + 1; j < a.length; j++) {
				sum = sum + a[j];

				if (sum == k)
					res++;
			}
		}

		return res;
	}

	public static void main(String[] args) {

		int[] a = { -3, 4, 5, 6, 2, 1 };
		int k = 6;
		
		System.out.println(  sum ( a,k));

	}

}
