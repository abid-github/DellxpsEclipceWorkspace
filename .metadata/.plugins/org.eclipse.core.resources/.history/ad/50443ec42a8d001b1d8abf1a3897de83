package com.algo.ds;

public class TripletsWithZeroSum {

	static void  findTriplets ( int []a, int n)
	{
		
		for ( int i = 0 ; i < a.length-2 ; i++)
		{
			for ( int j = i+1; j < a.length-1 ; j++)
			{
				for ( int k = i+2; k < a.length; k++)
				{
					
					
					
				}
			}
		}
		
		
		
		
		
		
	}
	
	
	public static void main(String[] args) {
		
		
		  int arr[] = {0, -1, 2, -3, 1};
		    int n =arr.length;
		    findTriplets(arr, n);
		
		

	}

}
