package com.algo.ds;

public class SumkOfSubArray {

	static int sum(int[] a, int k) {

		int res = 0;
	

		for (int i = 0; i < a.length; i++)

		{
			int sum = 0;
			for (int j = i; j < a.length; j++) {
				sum = sum + a[j];

				if (sum == k)
					res++;
			}
		}

		return res;
	}

	public static void main(String[] args) {

		int[] a = { 10, 2, -2, -20, 10 };
		int k = -10;

		System.out.println(sum(a, k));

	}

}
