package com.array;

import java.util.ArrayList;
import java.util.HashSet;

//separate unique, all duplicates, and unique duplicates values from array

public class DuplicatesInArray {

	public static void main(String[] args) {

		String[] sa = { "abid", "abid", "mango", "grapes", "apple", "abid", "abid", "mango" };

		ArrayList<String> unqlist = new ArrayList<String>();
		ArrayList<String> wholeduplist = new ArrayList<String>();
		HashSet<String> unqduplicates = new HashSet<String>();

		ArrayList<String> al = new ArrayList<String>();
		for (String data : sa) {
			al.add(data);
		}

		HashSet<String> hst = new HashSet<String>(al);

		System.out.println("Unique values" + hst);

		for (String data : sa) { 
			hst.add(data);

			if (unqlist.contains(data) == false) {
				unqlist.add(data);
				
			} else {
				wholeduplist.add(data);

				
			}
		}

		System.out.println("Unique values" + unqlist);

		System.out.println("All Duplicates" + wholeduplist);
		System.out.println("unqDuplicate values" + unqduplicates);
	}
}